  height: 0;
  padding-bottom: <%= 906.0 / 1920 * 100 %>%;
  position: relative;
}

.index-bg-1-wrapper {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.index-bg-title-1 {
  font-size: 40px;
  color: #fff;
  margin-left: -200px;
}

.index-bg-title-2 {
