}

.index-bg-text-2 {
  font-size: 20px;
  line-height: 26px;
  white-space: pre-wrap;
  text-align: center;
}

.index-bg-text-2:after {
  content: '';
  background: #63c8f3;
  display: block;
  margin: 20px auto 0;
  height: 4px;
  width: 40px;
}

.index-bg-2-wrapper-0 {
  display: flex;
  position: relative;
  justify-content: flex-end;
}

.index-bg-2-wrapper-1 {
  display: flex;
  flex-wrap: wrap;
  width: 420px;
  padding-top: 60px;
}

.index-bg-2-wrapper-2 {
  display: flex;
  position: absolute;
  left: 0;
  top: 0;
  width: 65%;
}

.index-bg-2-wrapper-3 {
  display: flex;
}

.index-bg-2-wrapper-4 {
  padding: 80px 30px 80px 0;
}

.index-bg-2-title {
  font-size: 30px;
  font-weight: bold;
  margin-bottom: 30px;
}

.index-bg-2-text {
  white-space: pre-wrap;
  font-size: 14px;
  line-height: 28px;
  margin-bottom: 40px;
}

.index-bg-2-link {
  background: #63c8f3;
  color: #fff;
  padding: 6px 14px;
  border-radius: 18px;
}

input[name="index-bg-2"] {
  display: none;
}

.index-bg-2-image {
  display: block;
  display: block;
  flex: 0 0 400px;
  width: 400px;
  height: 500px;
  background-size: auto 90%;
  background-repeat: no-repeat;
  background-position: center;
}

.index-bg-2-thumb {
  flex: 0 0 120px;
  width: 120px;
  height: 180px;
  background-size: auto 80%;
  background-repeat: no-repeat;
  background-position: center;
  background-color: #fff;
  position: relative;
  margin-bottom: 20px;
  margin-right: 20px;
}

label.index-bg-2-thumb {
  cursor: pointer;
}

.index-bg-2-thumb:after {
  content: attr(data-title);
  display: none;
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  padding: 6px 10px;
  text-align: center;
  background: #2a2e3f;
  color: #fff;
}

.index-bg-2-image-1,
.index-bg-2-thumb-1 {
  background-image: url(<%= image_path('index/product-slim.png') %>);
}

.index-bg-2-image-2,
.index-bg-2-thumb-2 {
  background-image: url(<%= image_path('index/product-pro.png') %>);
}

.index-bg-2-image-3,
.index-bg-2-thumb-3 {
  background-image: url(<%= image_path('index/product-plus.png') %>);
}

.index-bg-2-image-4,
.index-bg-2-thumb-4 {
  background-image: url(<%= image_path('index/product-life.png') %>);
}

.index-bg-2-image-5,
.index-bg-2-thumb-5 {
  background-image: url(<%= image_path('index/product-care.png') %>);
}

<% (1..5).each do |i| %>
  #index-bg-2-<%= i %>:checked ~ .index-bg-2-thumb-<%= i %>:after {
    display: block;
  }

  .index-bg-2-thumb-<%= i %>:hover:after {
    display: block;
  }

  .index-bg-2-wrapper-3-<%= i %> {
    display: none;
  }

  #index-bg-2-<%= i %>:checked ~ .index-bg-2-wrapper-2 .index-bg-2-wrapper-3-<%= i %> {
    display: flex;
  }

<% end %>

