  .products-index-bg-3 {
    height: auto;
    padding: 30px 0;
    background-size: cover;
  }

  .products-index-part-title-1 {
    font-size: 21px;
    margin-bottom: 10px;
  }

  .products-index-part-title-2 {
    font-size: 14px;
    margin-bottom: 25px;
