  .products-index-bg-3-5 {
    padding: 30px;
  }

  .products-index-product-text-1 {
    font-size: 22px;
  }

  .products-index-product-text-2 {
    font-size: 14px;
  }

  .products-index-product-text-3 {
    font-size: 14px;
    line-height: 28px;
  }

  .products-index-bg-4 {
    height: auto;
    padding: 30px 0 0;
  }

  .products-index-product {
    flex-direction: column;
  }

  .products-index-product-image-pro {
    width: 100%;
    height: 0;
    padding-bottom: <%= 586.0 / 627 * 100 %>%;
  }

  .products-index-product-image-slim {
    width: 100%;
    height: 0;
    padding-bottom: <%= 568.0 / 570 * 100 %>%;
