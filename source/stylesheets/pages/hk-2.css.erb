.cut-pic2,
.cut-pic3,
.cut-pic4,
.cut-pic5,
.cut-pic6,
.cut-pic7 {
  width: 100%;
  height: 0;
  padding-bottom: 30.20833333%;
  background-size: cover;
  background-position: center center;
  background-repeat: no-repeat;
}
.cut-pic9 {
  width: 100%;
  height: 0;
  padding-bottom: 23.95833333%;
  background-size: cover;
  background-position: center center;
  background-repeat: no-repeat;
}
.cut-pic1 {
  background-image: url(<%= image_path('pages/hk-2/pc/image-01.jpg') %>);
  order: 1;
}
.cut-pic2 {
  background-image: url(<%= image_path('pages/hk-2/pc/image-02.jpg') %>);
  order: 2;
}
.cut-pic3 {
  background-image: url(<%= image_path('pages/hk-2/pc/image-03.jpg') %>);
  order: 3;
}
.cut-pic4 {
