}
#info-form {
  background: #fff;
  padding: 1rem 3rem;
  order: 8;
}
#info-form .box {
  width: 100%;
  margin: 0 auto;
}
#info-form .box .text {
  font-size: .6rem;
  color: #313131;
  margin-bottom: .5rem;
}
#info-form .box .form {
  display: flex;
  -webkit-display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
  margin-bottom: 1rem;
}
#info-form .box .form-item-row {
  width: 45%;
  margin-right: 0.6rem;
}
#info-form .box .form-item {
  margin-right: 0.6rem;
  font-size: .3rem;
}
#info-form .box .form-item p {
  margin: .3rem 0 .2rem .3rem;
  color: #535353;
}
#info-form .box .ab-placeholder {
  position: absolute;
  left: 10px;
  top: 50%;
  transform: translateY(-50%);
  -webkit-transform: translateY(-50%);
  transition: all 0.3s;
  -webkit-transition: all 0.3s;
  font-size: 0.28rem;
  color: #535353;
}
#info-form .box .ab-placeholder-focus {
  position: absolute;
  left: 10px;
  top: 50%;
  font-size: 0.28rem;
  color: #afafaf;
  top: 0.1rem;
  transform: translateY(0);
  -webkit-transform: translateY(0);
}
#info-form .box .form-input {
  width: 100%;
  height: 1rem;
  line-height: .76rem;
  padding: 0 17% 0 10px;
  color: #535353 !important;
  background-color: #ffffff !important;
  border-radius: 3px;
  box-sizing: border-box;
  border: 1px solid #BFBFBF;
  font-size: .32rem;
  -webkit-appearance: none;
  appearance: none;
}
#info-form .box .input-box {
  font-size: 0;
  display: flex;
  display: -webkit-flex;
  align-items: center;
  -webkit-align-items: center;
  position: relative;
}
#info-form .box .submit-btn-box {
  -webkit-appearance: none;
  cursor: pointer;
  width: 30%;
  height: .8rem;
  line-height: .8rem;
  background-color: #40b6e6;
  -webkit-background-color: #40b6e6;
  border-radius: 4px;
  color: #FEFEFE;
  font-size: .4rem;
  margin: 0 auto;
  display: block;
  border: 0;
}
#info-form .box .submit-btn-box:disabled {
  background-color: #ccc;
}
